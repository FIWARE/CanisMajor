{
  "name": "canismajor",
  "version": "0.0.1",
  "scripts": {
    "dev": "nodemon --exec babel-node src/bin/www --verbose",
    "postinstall": "npm run build",
    "healthcheck": "node src/bin/health.js",
    "build": "rm -rf dist && babel src --out-dir dist && babel src/bin/* --out-dir dist && mv dist/src/bin dist && rm -rf dist/src",
    "start": "node dist/bin/www.js",
    "test": "mocha --require babel-core/register --require babel-polyfill --recursive --exit",
    "create": "cd sequelize  && ../node_modules/.bin/sequelize --env development db:create",
    "migrate": "cd sequelize  && ../node_modules/.bin/sequelize --env development db:migrate",
    "seed": "cd sequelize && ../node_modules/.bin/sequelize --env development db:seed:all",
    "dbinit": "node ./sequelize/create.js && npm run migrate"
  },
  "dependencies": {
    "@iota/converter": "^1.0.0-beta.30",
    "@iota/core": "^1.0.0-beta.30",
    "@iota/mam": "^0.7.3",
    "aeicontract": "0.0.2",
    "app-root-path": "^2.2.1",
    "body-parser": "^1.19.0",
    "chai-http": "^4.3.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.4",
    "ethereumjs-tx": "^2.1.2",
    "ethereumjs-util": "^7.1.0",
    "express": "^4.17.1",
    "gulp": "^4.0.2",
    "http-status-codes": "^2.1.4",
    "ipfs-http-client": "^50.1.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "merkletreejs": "^0.2.22",
    "moment": "^2.29.1",
    "mysql2": "^2.2.5",
    "nested-keys": "0.0.2",
    "node-fetch": "^2.6.1",
    "sequelize": "^6.6.5",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.3",
    "supertest-as-promised": "^4.0.2",
    "uuid": "^8.3.2",
    "web3": "^1.4.0",
    "web3-eth-contract": "^1.4.0",
    "winston": "^3.3.3",
    "xmlhttprequest": "1.8.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.3.4",
    "mocha": "^8.4.0",
    "nodemon": "^2.0.9"
  }
}
